{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": [
      "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
      "no-console": "off",
      "import/prefer-default-export": "off",
      "no-unused-vars": "off",
      "no-undef": "off",
      "no-useless-constructor": "off"
  }
}
